{
  "name": "iframe-coordinator",
  "version": "5.5.2",
  "description": "Tools for coordinating embedded apps via iframes.",
  "dependencies": {
    "decoders": "1.15.0"
  },
  "type": "module",
  "files": [
    "dist/client.js",
    "dist/host.js",
    "dist/index.js",
    "dist/**/*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@types/jasmine": "^3.10.6",
    "glob": "^7.2.0",
    "i": "^0.3.7",
    "jasmine-core": "^3.99.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.3",
    "lint-staged": "^13.0.3",
    "prettier": "^1.19.1",
    "puppeteer": "^1.20.0",
    "rollup": "^3.3.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^9.3.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.16.0",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build": "npm run build-lib",
    "build-lib": "rollup -c",
    "doc": "typedoc",
    "format.fix": "prettier --fix **/*.ts **/*.tsx",
    "lint.fix": "npm run lint.ts -- --fix",
    "lint.format": "prettier --check **/*.ts **/*.tsx",
    "lint": "tslint --project tsconfig.json && npm run lint.format",
    "start": "rollup -c --watch",
    "test": "karma start --single-run",
    "test.watch": "karma start",
    "test.watch.chrome": "karma start --browsers=Chrome",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write"
    ]
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:purecloudlabs/iframe-coordinator.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
