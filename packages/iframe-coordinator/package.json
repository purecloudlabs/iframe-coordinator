{
  "name": "iframe-coordinator",
  "version": "5.5.4",
  "description": "Tools for coordinating embedded apps via iframes.",
  "dependencies": {
    "decoders": "1.15.0"
  },
  "type": "module",
  "files": [
    "dist/client.js",
    "dist/host.js",
    "dist/index.js",
    "dist/**/*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/jasmine": "^5.1.4",
    "cross-var": "^1.1.0",
    "jasmine-core": "^5.1.1",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-typescript": "^5.5.4",
    "puppeteer": "^1.20.0",
    "rollup": "^4.9.1",
    "typedoc": "^0.25.4",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "npm run build-lib",
    "build-lib": "rollup -c",
    "current-version": "cross-var \"echo $npm_package_version\"",
    "doc": "typedoc",
    "start": "rollup -c --watch",
    "test": "karma start --single-run",
    "test.watch": "karma start",
    "test.watch.chrome": "karma start --browsers=Chrome"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:purecloudlabs/iframe-coordinator.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}
